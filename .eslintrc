{
	"env": {
		"browser": true,
		"commonjs": true,
		"es2021": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
		"ecmaVersion": 12
	},
	"rules": {
		"brace-style": ["error", "stroustrup"],
		"indent": ["error", "tab"],
		"semi": ["error", "always"],
		"quotes": ["error", "double"]
	}
}